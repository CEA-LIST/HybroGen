// Simple addition between variables which are
// 2 vectors composed of 16 values of 8 bits complex numbers
code * genAddComplex(code * ptr, int arch, int variant, sValue o0, sValue i0) // C compilette prototype
{
  #[
  flt 8 16 add (cpl 8 16 a, cpl 8 16 b)
    {
      cpl 8 16 r;
      a = 3 + a;
      a = b + 3.14;
      return a * a +b;
    }
  ]#
}

